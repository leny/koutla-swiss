{
    "source": "./src",
    "destination": "./docs",
    "plugins": [
        {
            "name": "esdoc-standard-plugin",
            "option": {
                "brand": {
                    "title": "Koutla Swiss",
                    "description": "CSS-in-JS toolkit, inspired by Kouto-Swiss",
                    "repository": "https://github.com/leny/koutla-swiss",
                    "site": "https://koutla-swiss.js.org",
                    "author": "https://leny.me"
                }
            }
        },
        {
            "name": "esdoc-ecmascript-proposal-plugin",
            "option": {
                "all": true
            }
        }
    ]
}
